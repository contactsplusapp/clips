{
    "name": "LinkedIn",
    "enabled": false,
    "handles": [
        "linkedin.com",
        "www.linkedin.com"
    ],
    "socialUsername": {
        "pathIndex": 2
    },
    "prep": {
        "scroll": {
            "to": 1000,
            "waitForSelector": ".pv-position-entity__logo"
        }
    },
    "fields": {
        "name": {
            "value": {
                "selector": ".pv-top-card-section__name"
            }
        },
        "photo": {
            "value": {
                "selector": ".pv-top-card-section__photo > img",
                "attribute": "src"
            }
        },
        "generalLocation": {
            "value": {
                "selector": ".pv-top-card-section__location"
            }
        },
        "title": {
            "value": {
                "selector": ".experience-section .pv-entity__summary-info > h3"
            }
        },
        "company": {
            "value": {
                "selector": ".experience-section .pv-entity__summary-info > h4 > .pv-entity__secondary-title"
            }
        }
    }
}