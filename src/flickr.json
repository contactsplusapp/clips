{
    "name": "flickr",
    "enabled": true,
    "handles": [
        "www.flickr.com"
    ],
    "socialUsername": {
        "selector": ".subtitle"
    },
    "fields": {
        "name": {
            "value": {
                "selector": ".title-block-content .title > h1"
            }
        },
        "photo": {
            "value": {
                "selector": ".avatar",
                "attribute": "style.backgroundImage",
                "regex": "\\(([^\\)]+)\\)"
            }
        }
    }
}